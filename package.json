{
  "name": "sls-template-typescript",
  "version": "1.0.0",
  "main": "serverless.yml",
  "repository": "https://github.com/jhon-birreo/sls-template-typescript.git",
  "author": "jhonasbn <jbirreo.n96@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --maxWorkers=5%",
    "debug": "export SLS_DEBUG=* && node --inspect /usr/local/bin/serverless offline",
    "offline": "sls offline --stage=local",
    "dynamodb:migrate": "sls dynamodb migrate --stage=local",
    "deployLocal": "sls deploy --verbose",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.326.0",
    "@aws-sdk/client-s3": "^3.326.0",
    "@aws-sdk/lib-dynamodb": "^3.326.0",
    "@middy/core": "^4.4.0",
    "@middy/http-error-handler": "^4.4.0",
    "@middy/http-event-normalizer": "^4.4.0",
    "@middy/http-json-body-parser": "^4.4.0",
    "@serverless/typescript": "^3.29.0",
    "@types/uuid": "^9.0.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1368.0",
    "uuid": "^9.0.0",
    "uuid-validate": "^0.0.3",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "esbuild": "^0.17.18",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "serverless": "^3.30.1",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
